@page
@model ActividadesSugeridasRazorPages.Pages.ActividadesSugeridasEstatus.DeleteModel

@{
    ViewData["Title"] = "Delete";
}


<h3>Esta seguro que desea borrar este Estatus?</h3>
<div>
    
    <hr />
    <dl>
        <dt>
            Fecha Registro
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.FechaEstatus)
        </dd>
        <dt>
            Actual
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.Actual)
        </dd>
        <dt>
            Observación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.Observacion)
        </dd>
        <dt>
            Descripción
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.ActividadesSugeridas.DesActividad)
        </dd>
        <dt>
            Tipo Actividad
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.TipoActividadesSugeridas.DesTipoActividadSugerida)
        </dd>
        <dt>
            Tipo Estatus
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ActividadSugeridaEstatus.TiposEstatus.DesTipoEstatus)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="ActividadSugeridaEstatus.IdEstatusDet" />
        <input type="submit" value="Borrar" class="btn btn-default" /> |
        <a class="btn btn-info btn-md active" role="button" asp-page="./Index" 
           asp-route-id="@Model.idAct">Regresar</a>
    </form>
</div>
